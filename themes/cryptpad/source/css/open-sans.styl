lesscss-percentage(n)
  (n * 100)%
/* Open Sans @font-face kit */
OpenSansPath = "fonts/open-sans-fontface/fonts"
/* BEGIN Light */
@font-face
  font-family 'Open Sans'
  src OpenSansPath + '/Light/OpenSans-Light.eot'
  src OpenSansPath + '/Light/OpenSans-Light.eot?#iefix' format('embedded-opentype'), OpenSansPath + '/Light/OpenSans-Light.woff' format('woff'), OpenSansPath + '/Light/OpenSans-Light.ttf' format('truetype'), OpenSansPath + '/Light/OpenSans-Light.svg#OpenSansLight' format('svg')
  font-weight 300
  font-style normal
/* END Light */
/* BEGIN Light Italic */
@font-face
  font-family 'Open Sans'
  src OpenSansPath + '/LightItalic/OpenSans-LightItalic.eot'
